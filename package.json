{
  "name": "ask-md",
  "displayName": "Ask.md",
  "description": "Quick implementation of what I think Yacine's Ask.md extension behaves like",
  "version": "0.0.1",
  "publisher": "yash-singh",
  "repository": "https://github.com/Yash-Singh1/ask-md",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ask-md.complete-8b",
        "title": "Ask.md: LLM Basic Completion Request"
      },
      {
        "command": "ask-md.complete-70b",
        "title": "Ask.md: LLM Advanced Completion Request"
      },
      {
        "command": "ask-md.set-api-key",
        "title": "Ask.md: Set Groq API Key"
      },
      {
        "command": "ask-md.delete-api-key",
        "title": "Ask.md: Delete Groq API Key"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint \"src/**/*.ts\"",
    "watch": "tsc -watch -p ./",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/node": "^16.18.34",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.26.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "openai": "^4.46.1"
  }
}
